<div class="row">
 <div class="col-md-2">
          
 </div>
 <div class="col-md-6">

<h1>Listing Tags</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= link_to tag.name, tag  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>
</div>
